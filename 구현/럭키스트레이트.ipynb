   "source": [
    "N = input()\n",
    "length = len(N)\n",
    "summury = 0\n",
    "\n",
    "for i in range(length//2):\n",
    "    summury += int(N[i])\n",
    "\n",
    "for i in range(length//2,length):\n",
    "    summury -= int(N[i])\n",
    "    \n",
    "if summury == 0 :\n",
    "    print(\"Lucky\")\n",
    "    \n",
    "else : \n",
    "    print(\"Ready\")"
   ]
